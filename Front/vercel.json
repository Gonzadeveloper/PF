{
    "version": 2,
    "builds": [
      {
        "src": "build/**/*.+(js|css|html)",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }